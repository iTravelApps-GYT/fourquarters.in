    <script>

        $(".gallery a img").first().css({ "margin-top": "-18px", "opacity": "0.5" });


        $("#speak1").html($(".gallery a img").first().parent().attr("data-span1"));
        $("#speak2").html($(".gallery a img").first().parent().attr("data-span2"));

        $(".gallery a img").mouseover(function () {
            $(".gallery a img").each(function (prop, value) {
                $(this).css({ "margin-top": "0px", "opacity": "1" });

            });
            $(this).css({ "margin-top": "-18px", "opacity": "0.5" });

            $("#speak1").html($(this).parent().attr("data-span1"));
            $("#speak2").html($(this).parent().attr("data-span2"));



        });

        $("html").removeClass(function () {
            return $(this).attr("class");
        });

        $("#ThinkDDL li a").click(function () {
            if ($(this).attr("data-flag2") === "false") {
                $("#ThinkDDL li a").each(function (prop, val) {
                    var el = $(this).attr("data-think2");
                    $("#" + el).hide(1000);
                    $(this).attr("data-flag2", "false");
                });

                var el = $(this).attr("data-think2");
                $("#" + el).show(1000);
                $(this).attr("data-flag2", "true");
            }
            return false;

        });

        $("#HomeDDL ul li a").click(function () {
            $(this).removeClass()
        });

        $("#DDDigital li a").click(function () {
            if ($(this).attr("data-flag1") === "false") {
                $("#DDDigital li a").each(function (prop, val) {
                    var el = $(this).attr("data-think1");
                    var el2 = $(this).attr("data-id");
                    $("#" + el).hide(1000);
                    $("#" + el2).hide(1000);
                    $(this).attr("data-flag1", "false");
                });

                var el = $(this).attr("data-think1");
                var el2 = $(this).attr("data-id");
                $("#" + el).show(1000);
                $("#" + el2).show(1000);
                $(this).attr("data-flag1", "true");
            }
            return false;

        });

        var el3 = $("#DDDigital li a").first().attr("data-think1");
        var el4 = $("#DDDigital li a").first().attr("data-id");
        //alert(el3);
        //alert(el4);
        $("#" + el3).show(1000);
        $("#" + el4).show(1000);
        $("#DDDigital li a").first().attr("data-flag1", "true");

        var el1 = $("#ThinkDDL li a").first().attr("data-think2");
        //alert(el1);
        $("#" + el1).show(1000);
        $("#ThinkDDL li a").first().attr("data-flag2", "true");


    </script>

